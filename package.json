    "eslint-config-prettier": "^9.1.2",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.0",
    "tsup": "^8.5.0",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.11.1",
